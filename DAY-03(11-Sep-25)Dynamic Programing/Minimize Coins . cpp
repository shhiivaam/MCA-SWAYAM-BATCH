#include <bits/stdc++.h>
using namespace std;
const int INF = 1e9;

int Solve(int val, vector<int> &coins, vector<int> &dp) {
    if (val == 0) return 1; 
    if (val < 0) return INF;
    if (dp[val] != -1) return dp[val];

    int ans = INF;
    for (int c : coins) {
        if (val - c >= 0) {
            ans = min(ans, 1 + Solve(val - c, coins, dp));
        }
    }
    return dp[val] = ans;
}

void solve() {
    int n, val;
    cin >> n >> val;
    vector<int> coins(n);
    for (int i = 0; i < n; i++) cin >> coins[i];

    vector<int> dp(val + 1, -1);
    int ans = Solve(val, coins, dp);
    cout << (ans >= INF ? -1 : ans) << "\n";
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    solve();
    return 0;
}
